<div class="container">
	<div class="row">
		<div class="col-md-8">
			<div class="panel panel-default">
  			<div class="panel-body">
		<table class="table">
			<h2>Developer List</h2>
			<thead><tr>
				<th>Name</th>
				<th>Edit</th>
				<th>Last updated</th>
			</tr></thead>
			<tbody>
			<% @developer.each do |developer| %>
				<tr>
				<td><%= link_to developer.name, developer_path(developer) %> </td>
				<td><%= link_to 'Edit',edit_developer_path(developer) %></td>
				<td><%= time_ago_in_words(developer.updated_at) %></td>
				</tr>
			<% end %>
			</tbody>
		</table>
			</div></div>
			<%= will_paginate @developer, renderer: BootstrapPagination::Rails %>
		</div>
	</div>
</div>