<div class='nested-fields'>
	<div class="col-md-12">
		<h3> Character Creation</h3>
	</div>
		 <div class="col-md-2">
		 	<%= f.input :name,required: false   %>
		 </div>
		
		 <div class="col-md-3">
		 	<%= f.input :remote_img_string_url %>
		 </div>
		 
		  <div class="col-md-2">
		 	<%= f.input :img_string %>
		 </div>

		 <div class="col-md-2">
		 	<%= f.input :role %>
		 </div>
		
		 <div class="col-md-12">
		 	<%= f.input :summary,input_html: { style: "height:150px;" }  %>
		 </div>

	<div class="col-lg-3">
  	 	 <%= link_to_remove_association "Remove Character", f,:class => 'btn btn-default'  %>
  	 </div>
   	
 </div>