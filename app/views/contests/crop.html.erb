<% title 'Crop Image' %>
<div class="container">
	<div class="row">
		<div class="col-md-12">
<%= simple_form_for @contest do |f| %>
	
  <%= f.simple_fields_for :contest_characters,@character do |c| %>
	  <%= c.cropbox :image %>

	  <%= c.cropbox :details %>
	  <%= c.submit 'Crop',:class => "btn btn-danger",:style =>'margin-top: 50px; width: 120px;' %>
  <% end %>
<% end %>
</div>
</div></div>